                    <string notr="true">/*顶部导航区域*/
#qt_calendar_navigationbar {
    background-color: white;
    min-height: 10px;
	max-height: 1000px;
}


/*上一个月按钮和下一个月按钮*/
#qt_calendar_prevmonth, #qt_calendar_nextmonth {
	padding:0px;
    border: none; /*去掉边框*/
    color: black;
	min-width: 20px;
    max-width: 20px;
    min-height: 16px;
    max-height: 16px;
    border-radius: 6px; /*看来近似椭圆*/
    font-weight: bold; /*字体加粗*/
	    qproperty-icon: none; /*去掉默认的方向键图片，当然也可以自定义*/
    background-color: transparent;/*背景颜色透明*/
}

#qt_calendar_prevmonth {
	font-size:20px;
    qproperty-text: &quot;&lt;&quot;; /*修改按钮的文字*/
}
#qt_calendar_nextmonth {
font-size:20px;
    qproperty-text: &quot;&gt;&quot;;
}
#qt_calendar_prevmonth:hover, #qt_calendar_nextmonth:hover {
    background-color: rgba(0, 0, 0, 100);
}
#qt_calendar_prevmonth:pressed, #qt_calendar_nextmonth:pressed {
    background-color: rgba(0, 0, 0, 100);
}


/*年,月控件*/
#qt_calendar_yearbutton, #qt_calendar_monthbutton {
    color: black;
    margin: 10px;
    min-width: 60px;
    border-radius: 30px;

	font: 700 14pt &quot;微软雅黑&quot;;
}
#qt_calendar_yearbutton:hover, #qt_calendar_monthbutton:hover {
    background-color: rgba(225, 225, 225, 100);
}
#qt_calendar_yearbutton:pressed, #qt_calendar_monthbutton:pressed {
    background-color: rgba(235, 235, 235, 100);
}


/*年份输入框*/
#qt_calendar_yearedit {
    min-width: 50px;
    color: white;
    background: transparent;/*让输入框背景透明*/
}
#qt_calendar_yearedit::up-button { /*往上的按钮*/
    width: 20px;
    subcontrol-position: right;/*移动到右边*/
}
#qt_calendar_yearedit::down-button { /*往下的按钮*/
    width: 20px;
    subcontrol-position: left; /*移动到左边去*/
}


/* 月份选择菜单 */
QCalendarWidget QToolButton QMenu {
     background-color: white;
}

/* 菜单项样式 */
QCalendarWidget QToolButton QMenu::item {
    padding: 10px;
}

QCalendarWidget QToolButton QMenu::item:selected:enabled {
    background-color: rgb(230, 230, 230);
}

/* 去掉月份选择小箭头 */
QCalendarWidget QToolButton::menu-indicator {
	image: none;
    subcontrol-position: right center;
}


/*下方的日历表格*/
#qt_calendar_calendarview::item:selected {
    background-color: rgb(0, 188, 212); /* 选中的背景颜色 */
    color: white; /* 字体变白 */
    border-radius: 15px; /* 让背景变成圆形 */
    padding: 0px 0px; /* 调整内边距：上下 2px，左右 8px */
    margin: 16px 6px; /* 上下减少间距 */
	outline: none;
}

#qt_calendar_calendarview {
    outline: none; /* 去掉选中后的虚线框 */
}

/* 日历栏 */
QCalendarWidget QTableView {
